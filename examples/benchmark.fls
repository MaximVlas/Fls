fun fib(n) {
  if (n < 2) {
    return n;
  }
  return fib(n - 2) + fib(n - 1);
}

var n = 35;

print "Running FLS benchmark...";
var startTime = clock();
var result = fib(n);
var endTime = clock();

var elapsedTime = endTime - startTime;

print "FLS Benchmark Complete:";
print "fib(35) is";
print result;
print "Calculation took:";
print elapsedTime;
print "seconds.";
