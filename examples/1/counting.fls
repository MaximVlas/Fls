var startTime = clock();

for (var i = 0; i <= 1000000; i = i + 1) {
  println (i);
}

var endTime = clock();
var elapsedTime = endTime - startTime;

println ("Done counting!");
println ("The loop took:");
println (elapsedTime);
println ("seconds.");


