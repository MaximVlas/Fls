println("--- Testing Math Library ---");

// Import the new Fls math library
import "std/math.fls";

// --- Test Fls-based functions ---
println("sign(10) = " + toString(sign(10)));       // Expected: 1
println("sign(-5) = " + toString(sign(-5)));      // Expected: -1
println("sign(0) = " + toString(sign(0)));         // Expected: 0
println("degrees(PI) = " + toString(degrees(PI))); // Expected: 180
println("radians(180) = " + toString(radians(180))); // Expected: 3.1415...
println("max(10, 20) = " + toString(max(10, 20)));   // Expected: 20
println("min(-1, 5) = " + toString(min(-1, 5)));     // Expected: -1

// --- Test C-based native functions ---
println("sqrt(16) = " + toString(sqrt(16)));       // Expected: 4
println("pow(2, 8) = " + toString(pow(2, 8)));         // Expected: 256
println("floor(3.9) = " + toString(floor(3.9)));     // Expected: 3
println("ceil(4.1) = " + toString(ceil(4.1)));       // Expected: 5
println("round(4.5) = " + toString(round(4.5)));     // Expected: 5

// --- Test Number Theory functions ---
println("factorial(5) = " + toString(factorial(5))); // Expected: 120
println("gcd(48, 18) = " + toString(gcd(48, 18)));   // Expected: 6
println("lcm(21, 6) = " + toString(lcm(21, 6)));     // Expected: 42
